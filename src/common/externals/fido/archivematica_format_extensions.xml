<?xml version="1.0"?>
<formats xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" xsi:noNamespaceSchemaLocation="fido-formats.xsd" version="0.3">
  <format>
    <puid>archivematica-fmt/2</puid>
    <name>AccessData AD1 Disk Image</name>
    <extension>ad1</extension>
    <signature>
      <name>AccessData AD1 Disk Image</name>
      <pattern>
        <position>BOF</position>
        <regex>ADSEGMENTEDFILE</regex>
      </pattern>
    </signature>
  </format>

  <format>
    <puid>archivematica-fmt/3</puid>
    <name>AccessData AD1 Encrypted Disk Image</name>
    <extension>ad1</extension>
    <signature>
      <name>AccessData AD1 Encrypted Disk Image</name>
      <pattern>
        <position>BOF</position>
        <regex>ADCRYPT</regex>
      </pattern>
    </signature>
  </format>

  <format>
    <puid>archivematica-fmt/4</puid>
    <name>Raw disk image</name>
    <extension>dd</extension>
    <extension>dd.001</extension>
    <extension>001</extension>
  </format>

  <format>
    <puid>archivematica-fmt/5</puid>
    <name>Advanced Forensic Format</name>
    <extension>aff</extension>
    <signature>
      <name>Advanced Forensic Format</name>
      <pattern>
        <position>BOF</position>
        <regex>AFF10\r\n\x00</regex>
      </pattern>
    </signature>
  </format>
</formats>
