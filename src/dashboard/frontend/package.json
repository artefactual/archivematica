{
  "name": "dashboard",
  "private": true,
  "version": "0.0.0",
  "description": "Dashboard components (Appraisal tab and Transfer browser) for Archivematica",
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/plugin-transform-modules-commonjs": "^7.25.7",
    "@babel/plugin-transform-object-assign": "^7.25.7",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "angular-gettext-cli": "^1.2.0",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "html-loader": "^5.1.0",
    "http-server": "^14.1.1",
    "imports-loader": "^5.0.0",
    "jasmine-core": "^5.4.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-firefox-launcher": "^2.1.3",
    "karma-jasmine": "^5.1.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-webpack": "^5.0.1",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.47",
    "shelljs": "^0.8.5",
    "style-loader": "^4.0.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "extract-messages": "angular-gettext-cli --files \"./app/**/*.+(js|html)\" --dest \"./app/locale/extract.pot\" --marker-name \"i18n\"",
    "prepare": "webpack --entry ./app.js",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -c-1",
    "pretest": "npm install",
    "test": "karma start test/karma.conf.js",
    "test-single-run": "karma start test/karma.conf.js  --single-run"
  },
  "dependencies": {
    "angular": "~1.5.0",
    "angular-cookies": "~1.5.11",
    "angular-gettext": "^2.3.8",
    "angular-loader": "~1.5.0",
    "angular-mocks": "~1.5.0",
    "angular-route": "~1.5.0",
    "angular-route-segment": "~1.5.0",
    "angular-tree-control": "^0.2.0",
    "angular-ui-bootstrap": "^0.14.3",
    "angular-ui-validate": "^1.2.2",
    "archivematica-browse-helpers": "git+https://github.com/artefactual-labs/archivematica-browse-helpers.git",
    "base64-helpers": "github:artefactual-labs/base64-helpers#v0.1.2",
    "d3": "^3.5.17",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "restangular": "^1.6.1"
  },
  "overrides": {
    "angular-gettext-tools": {
      "cheerio": "^1.0.0-rc.12"
    }
  }
}
