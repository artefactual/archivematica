{
  "name": "dashboard",
  "private": true,
  "version": "0.0.0",
  "description": "Dashboard components (Appraisal tab and Transfer browser) for Archivematica",
  "devDependencies": {
    "angular-gettext-cli": "^1.2.0",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "http-server": "^0.10.0",
    "imports-loader": "^0.6.5",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "~0.13",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.6",
    "karma-junit-reporter": "^0.2.2",
    "karma-webpack": "^1.7.0",
    "ng-cache-loader": "^0.0.26",
    "shelljs": "^0.2.6",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.15.0"
  },
  "scripts": {
    "extract-messages": "angular-gettext-cli --files \"./app/**/*.+(js|html)\" --dest \"./app/locale/extract.pot\" --marker-name \"i18n\"",
    "prepare": "webpack --progress --colors --entry ./app.js",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -c-1",
    "pretest": "npm install",
    "test": "karma start test/karma.conf.js",
    "test-single-run": "karma start test/karma.conf.js  --single-run"
  },
  "dependencies": {
    "angular": "~1.5.0",
    "angular-cookies": "~1.5.11",
    "angular-gettext": "^2.3.8",
    "angular-loader": "~1.5.0",
    "angular-mocks": "~1.5.0",
    "angular-route": "~1.5.0",
    "angular-route-segment": "~1.5.0",
    "angular-tree-control": "git+https://github.com/wix/angular-tree-control.git#273a0422d28e667784135b347c0aa06eaa1b2cd2",
    "angular-ui-validate": "^1.2.2",
    "archivematica-browse-helpers": "git+https://github.com/artefactual-labs/archivematica-browse-helpers.git",
    "base64-helpers": "git+https://github.com/artefactual-labs/base64-helpers.git",
    "d3": "^3.5.12",
    "font-awesome": "^4.3.0",
    "lodash": "^4.5.1",
    "moment": "^2.21.0",
    "restangular": "^1.5.1"
  }
}
