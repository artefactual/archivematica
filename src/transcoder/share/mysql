def inPreservationFormat():
    ex=["PPT"
        "JP2", "PNG", \
        "SVG", "WAV", "TIF", "TIFF", "ODG", "ODP", "MKV", "MXF", "ODT", "MBOX", "MBOXI", "AI", \
    "PPTX", \
    "DNG" ]
*/
