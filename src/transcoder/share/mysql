    '%outputDirectory%%prefix%%fileName%%postfix%.jpg',
    @convertEventDetailCommandID,
    @standardVerificationCommand,
    ('Transcoding to jpg with convert')
);
set @convertToJpg70percentCommandID = LAST_INSERT_ID();

